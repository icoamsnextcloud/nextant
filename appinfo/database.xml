<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>

	<table>
		<name>*dbprefix*nextant_live_queue</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<notnull>true</notnull>
				<autoincrement>true</autoincrement>
				<unsigned>true</unsigned>
				<primary>true</primary>
				<length>8</length>
			</field>
			<field>
				<name>item</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>512</length>
			</field>
		</declaration>
	</table>

	<!--
	    Author: Lawrence Chan
	    Description: Exclusion List Table
	-->
	<table>
		<name>*dbprefix*nextant_exclusion_list</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<notnull>true</notnull>
				<autoincrement>true</autoincrement>
				<unsigned>true</unsigned>
				<primary>true</primary>
				<length>8</length>
			</field>
			<field>
				<name>fileid</name>
				<type>integer</type>
				<notnull>true</notnull>
				<length>11</length>
			</field>
			<field>
				<name>owner</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>100</length>
			</field>
			<field>
				<name>path</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>10000</length>
			</field>
		</declaration>
	</table>

</database>